{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "global-require": 0,
    "consistent-return": 0,
    "import/no-dynamic-require": 0,
    "no-path-concat": 0,
    "prefer-template": 0,
    "no-unused-vars": 0
  }
}
